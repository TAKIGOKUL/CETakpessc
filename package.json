  "version": "0.0.0",
  "type": "module",
  "homepage": "https://tecrade.github.io/AKPESSC-25",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
